<div id="div-tabela" class="table-responsive" style="height: 300px">
    <table class="table table-bordered table-striped" id="tabela" style="background-color: #f2f2f2; color: #333;">
      <thead>
        <tr>              
          <th style="width: 30%;">Nome</th>
          <th style="width: 20%">Calorias (kcal)</th>
          <th style="width: 15%">Prote√≠nas (g)</th>
          <th style="width: 20%">Carboidrados (g)</th>        
          <th style="width: 15%">Gorduras (g)</th>      
        </tr>
      </thead>
      <tbody id="table-body">  
        <% if(alimentos.length > 0 ){ %> 
          <% alimentos.forEach(a =>{ %>
            <tr>
              <td> <%= a[0] %> </td>
              <td> <%= a[1].Calorias %> </td>
              <td> <%= a[1].Proteinas %> </td>
              <td> <%= a[1].Carboidratos %> </td>
              <td> <%= a[1]['Gorduras Totais'] %> </td>
            </tr>
          <% }) %>
        <% } %>        
      </tbody>
    </table>
</div>